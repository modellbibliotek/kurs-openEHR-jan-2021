                                                        <term_definitions code="at0000">
                                                            <items id="text">Inandad syrgas</items>
                                                            <items id="description">Mängden syre som ges eller ordineras till patienten, angivet som andel, procent, eller indirekt som syrgasflöde.</items>
                                                        </term_definitions>
                                                        <term_definitions code="at0051">
                                                            <items id="text">Syrgasflöde</items>
                                                            <items id="description">Syrgasflöde (flödeshastighet) som ges till en individ.</items>
                                                            <items id="comment">Exempelvis "5 l/min"</items>
                                                        </term_definitions>
                                                        <term_definitions code="at0052">
                                                            <items id="text">FiO₂</items>
                                                            <items id="description">Andel syre i inandningsluften.</items>
                                                            <items id="comment">Exempelvis "0.28"</items>
                                                        </term_definitions>
                                                        <term_definitions code="at0053">
                                                            <items id="text">Procent O₂</items>
                                                            <items id="description">Procentandel syre i inandningsluften.</items>
                                                            <items id="comment">Exempelvis "24%"</items>
                                                        </term_definitions>
                                                        <term_definitions code="at0054">
                                                            <items id="text">Administrationssätt för syrgas</items>
                                                            <items id="description">Metoden för att ge (administrera) syrgasen.</items>
                                                            <items id="comment">Avsett för att registrera enkla beskrivningar, exempelvis "näsgrimma". Om mer detaljerad beskrivning krävs så kan fältet "Detaljer om administrationssätt" (ett "archetype SLOT") användas .&#13;
</items>
                                                        </term_definitions>
                                                        <term_definitions code="at0057">
                                                            <items id="text">På luft</items>
                                                            <items id="description">Patienten andas vanlig luft, motsvarande 21% O₂ eller 0.21 FiO₂ eller syrgasflöde 0 liter per minut</items>
                                                            <items id="comment">Om "på Luft" är valt (sant) så ska varken syrgasflöde, procent O₂ eller FiO₂ anges. Omvänt gäller att "på Luft" ej ska anges om syrgasflöde, procent O₂ eller FiO₂ anges.</items>
                                                        </term_definitions>
                                                        <term_definitions code="at0058">
                                                            <items id="text">Detaljer om administrationssätt</items>
                                                            <items id="description">Ytterligare detaljer om administrationssätt för syrgas.</items>
                                                            <items id="comment">Exempelvis detaljer om befuktning eller assisterad ventilation.</items>
