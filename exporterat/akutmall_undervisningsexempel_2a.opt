                        <term_definitions code="at0000">
                            <items id="text">Andning</items>
                            <items id="description">De observerade egenskaperna i spontanandning.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Historik</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Ospecificerad händelse</items>
                            <items id="description">Ospecificerad standardhändelse vid en tidpunkt eller inom ett tidsintervall som explicit kan definieras i en mall eller genereras automatiskt av vissa IT-system.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">List</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Frekvens</items>
                            <items id="description">Spontanandningens frekvens.</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Regelbundenhet</items>
                            <items id="description">Spontanandningens regelbundenhet.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Regelbunden</items>
                            <items id="description">Regelbunden andning.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Oregelbunden</items>
                            <items id="description">Oregelbunden andning.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Klinisk tolkning</items>
                            <items id="description">Ord, fras eller kort beskrivning som fångar den kliniska betydelsen hos andningsfynden.</items>
                            <items id="comment">Kodning med en terminologi rekommenderas där så är möjligt. Exempelvis normal andning, tecken på ansträngd andning, Kussmauls andning, Cheyne-Stokes andning, återkommande apné. Flera val är tillåtet.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Andningsdjup</items>
                            <items id="description">Spontanandningens djup.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Normal</items>
                            <items id="description">Normalt andningsdjup.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Ytlig</items>
                            <items id="description">Ytligt andningsdjup.</items>
                        </term_definitions>
                        <term_definitions code="at0019">
                            <items id="text">Djup</items>
                            <items id="description">Djupandning.</items>
                        </term_definitions>
                        <term_definitions code="at0022">
                            <items id="text">List</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0024">
                            <items id="text">Klinisk beskrivning</items>
                            <items id="description">Beskrivning av individens andningsdjup.</items>
                            <items id="comment">Exempelvis plågad andning, andningsrörelse med hjälp av accessorisk muskel eller intermittent apné; suckar, grymtningar, jämmer, flämtningar. När en fullständig respiratorisk undersökning genomförs dokumenteras den lämpligen som en del av undersökningsfynden.</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="text">Ojämn</items>
                            <items id="description">Ojämnt andningsdjup.</items>
                        </term_definitions>
                        <term_definitions code="at0037">
                            <items id="text">Ansträngning</items>
                            <items id="description">Detaljer om den fysiska ansträngningen vid undersökningen.</items>
                            <items id="comment">Individens ansträngningsnivå under eller strax innan observationen.</items>
                        </term_definitions>
                        <term_definitions code="at0055">
                            <items id="text">Tillförd syrgas</items>
                            <items id="description">Detaljer om mängden syrgas som tillförs individen vid observationstillfället.</items>
                            <items id="comment">Värden som antas är syrekoncentration på 21%, Fi02 på 0.21 och flödeshastighet av syre på 0 l/min eller 0 ml/min.</items>
                        </term_definitions>
                        <term_definitions code="at0056">
                            <items id="text">Möjliga felkällor</items>
                            <items id="description">Beskrivning av faktorer och felkällor som kan påverka bedömningen av andningen.&#13;
</items>
                            <items id="comment">Exempelvis ångestnivå, smärta, matning av spädbarn, trakeostomi.</items>
                        </term_definitions>
                        <term_definitions code="at0057">
                            <items id="text">*Tree(en)</items>
                            <items id="description">*@ internal @(en)</items>
                        </term_definitions>
                        <term_definitions code="at0058">
                            <items id="text">Tilläggsinformation</items>
                            <items id="description">Plats för att infoga tilläggsinformation som krävs för lokala anpassningar eller anpassning till andra referensmodeller eller formella krav.</items>
                            <items id="comment">Exempelvis lokala informationskrav eller metadata för anpassning till FHIR.</items>
                        </term_definitions>
                        <term_definitions code="at0062">
                            <items id="text">Förekomst</items>
                            <items id="description">Observation av spontanandning.</items>
                            <items id="comment">Det här fältet kan vara användbart i extrema situationer där formell mätning av andning ej är möjlig. Exempelvis om patienten är medvetslös och befinner sig på annan plats, såsom fast i en bil eller har fallit utför en klippa. Förekomst av andning kan antas om adningsfrekvensen är högre än 0 andetag per minut.</items>
                        </term_definitions>
                        <term_definitions code="at0063">
                            <items id="text">Förekommer</items>
                            <items id="description">Andningsrörelser observeras.</items>
                        </term_definitions>
                        <term_definitions code="at0064">
                            <items id="text">Ej möjlig att upptäcka</items>
                            <items id="description">Andningsrörelser kan inte upptäckas genom observation.</items>
                        </term_definitions>
                        <term_definitions code="at0065">
                            <items id="text">Kroppsställning</items>
                            <items id="description">Individens kroppsställning vid observationen.</items>
                        </term_definitions>
                        <term_definitions code="at0066">
                            <items id="text">Stående</items>
                            <items id="description">Individen stod, gick eller sprang.</items>
                        </term_definitions>
                        <term_definitions code="at0067">
                            <items id="text">Sittande</items>
                            <items id="description">Individen satt på exempelvis en säng eller en stol.</items>
                        </term_definitions>
                        <term_definitions code="at0068">
                            <items id="text">Lutande</items>
                            <items id="description">Individen lutade sig i ungefär 45 graders vinkel med benen i högläge i bäckennivå.</items>
                        </term_definitions>
                        <term_definitions code="at0069">
                            <items id="text">Liggande</items>
                            <items id="description">Individen låg på rygg.</items>
                        </term_definitions>
                        <term_definitions code="at0070">
                            <items id="text">Kommentar</items>
                            <items id="description">Kommentarer avseende andning som inte beskrivs i övriga fält.</items>
                        </term_definitions>
                        <term_definitions code="at0071">
                            <items id="text">Magläge</items>
                            <items id="description">Individen låg på mage.</items>
                        </term_definitions>
                    </children>
