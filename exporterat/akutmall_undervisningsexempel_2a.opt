                            <items id="text">Mjliga felkllor</items>
                            <items id="description">Beskrivning av faktorer som kan pverka tolkningen av de fysiska fynden.</items>
                            <items id="comment">Exempelvis pacemaker, ngestniv, smrta eller feber.</items>
                        </term_definitions>
                        <term_definitions code="at1019">
                            <items id="text">Metod</items>
                            <items id="description">Metod som anvnds fr att observera pulsen eller hjrtslagen.</items>
                            <items id="comment">Exempelvis stetoskop eller elektronisk vervakning.</items>
                        </term_definitions>
                        <term_definitions code="at1022">
                            <items id="text">Klinisk beskrivning</items>
                            <items id="description">Beskrivning av pulsen eller hjrtslagen.</items>
                        </term_definitions>
                        <term_definitions code="at1023">
                            <items id="text">Klinisk tolkning</items>
                            <items id="description">Enstaka ord, fras eller kort beskrivning om den kliniska innebrden och betydelsen av fynd frn puls eller hjrtslag, inklusive rytm.</items>
                            <items id="comment">Kodning med en terminologi r att fredra, om det r tillmpligt. Exempelvis bradykardi, prematura slag eller sinusrytm. Flera utltanden r tilltna.</items>
                        </term_definitions>
                        <term_definitions code="at1024">
                            <items id="text">Nrvaro</items>
                            <items id="description">Puls eller hjrtslag r identifierade.</items>
                        </term_definitions>
                        <term_definitions code="at1025">
                            <items id="text">Frnvaro</items>
                            <items id="description">Puls eller hjrtslag kan inte identifieras.</items>
                        </term_definitions>
                        <term_definitions code="at1028">
                            <items id="text">Oregelbunden</items>
                            <items id="description">Mnstret r oregelbundet.</items>
                        </term_definitions>
                        <term_definitions code="at1030">
                            <items id="text">Karaktr</items>
                            <items id="description">Beskrivning av pulsens eller hjrtslagets karaktr.</items>
                            <items id="comment">Kodning med en terminologi r nskvrd, om det r tillmpligt. Exempelvis *full, trdfin, kraftig, lngsamt stigande eller Corrigans puls.</items>
                        </term_definitions>
                        <term_definitions code="at1032">
                            <items id="text">Palpation</items>
                            <items id="description">Lkare eller sjukskterska observerar fynden genom fysisk berring av patienten.</items>
                        </term_definitions>
                        <term_definitions code="at1033">
                            <items id="text">Auskultation</items>
                            <items id="description">Fynden observeras med ett hjlpmedel, exempelvis ett stetoskop.</items>
                        </term_definitions>
                        <term_definitions code="at1034">
                            <items id="text">Automatisk, icke-invasiv</items>
                            <items id="description">Fynden observeras icke-invasivt med hjlp av utrustning ssom en pulsoximeter eller ett stetoskop.</items>
                        </term_definitions>
                        <term_definitions code="at1036">
                            <items id="text">Maximum</items>
                            <items id="description">Maxpuls eller maxfrekvens observerad under anstrngning.</items>
                        </term_definitions>
                        <term_definitions code="at1037">
                            <items id="text">Lokalisation</items>
                            <items id="description">Lokalisation dr pulsen eller hjrtslagen observerades.</items>
                        </term_definitions>
                        <term_definitions code="at1038">
                            <items id="text">Strlbensartr - Vnster</items>
                            <items id="description">Vnster strlbensartr.</items>
                        </term_definitions>
                        <term_definitions code="at1039">
                            <items id="text">Strlbensartr - Hger</items>
                            <items id="description">Hger strlbensartr.</items>
                        </term_definitions>
                        <term_definitions code="at1040">
                            <items id="text">Hjrt</items>
                            <items id="description">Hjrttrakten.</items>
                        </term_definitions>
                        <term_definitions code="at1041">
                            <items id="text">Halsartr - Vnster</items>
                            <items id="description">Vnster halsartr.</items>
                        </term_definitions>
                        <term_definitions code="at1042">
                            <items id="text">Halsartr - Hger</items>
                            <items id="description">Hger halsartr.</items>
                        </term_definitions>
                        <term_definitions code="at1043">
                            <items id="text">Lrbensartr - Vnster</items>
                            <items id="description">Vnster lrbensartr.</items>
                        </term_definitions>
                        <term_definitions code="at1044">
                            <items id="text">Lrbensartr- Hger</items>
                            <items id="description">Hger lrbensartr.</items>
                        </term_definitions>
                        <term_definitions code="at1047">
                            <items id="text">Finger</items>
                            <items id="description">Ett ospecificerat finger.</items>
                        </term_definitions>
                        <term_definitions code="at1048">
                            <items id="text">Armartr - Vnster</items>
                            <items id="description">Vnster armartr.</items>
                        </term_definitions>
                        <term_definitions code="at1049">
                            <items id="text">Armartr - Hger</items>
                            <items id="description">Hger armartr.</items>
                        </term_definitions>
                        <term_definitions code="at1050">
                            <items id="text">Automatisk, invasiv</items>
                            <items id="description">Fynden observeras invasivt med hjlp av utrustning ssom en arteriell kateter.</items>
                        </term_definitions>
                        <term_definitions code="at1051">
                            <items id="text">rsnibb</items>
                            <items id="description">En ospecificerad rsnibb.</items>
                        </term_definitions>
                        <term_definitions code="at1054">
                            <items id="text">T</items>
                            <items id="description">En ospecificerad t.</items>
                        </term_definitions>
                        <term_definitions code="at1055">
                            <items id="text">Oregelbunden typ</items>
                            <items id="description">Ett mer specifikt mnster av oregelbunden puls eller oregelbundna hjrtslag.</items>
                            <items id="comment">Ett val frn den hr vrdemngden r endast giltigt om "oregelbunden" vljs frn komponenten "regelbunden".</items>
                        </term_definitions>
                        <term_definitions code="at1056">
                            <items id="text">Tillggsinformation</items>
                            <items id="description">Plats fr att infoga tillggsinformation som krvs fr lokala anpassningar eller anpassning till andra referensmodeller eller formella krav.</items>
                            <items id="comment">Exempelvis lokala informationskrav eller metadata fr anpassning till FHIR- eller CIMI-motsvarigheter.</items>
                        </term_definitions>
                        <term_definitions code="at1057">
                            <items id="text">Klinisk tolkning</items>
                            <items id="description">Allmnt flt fr ett enskilt eller samtliga utltanden om pulsen eller hjrtrytmen.</items>
                        </term_definitions>
                        <term_definitions code="at1058">
                            <items id="text">Rytm</items>
                            <items id="description">Specifik slutsats om pulsens eller hjrtslagens rytm, hrledd frn en kombination av hjrtfrekvens, mnster och andra egenskaper som observerats vid underskningen.</items>
                        </term_definitions>
                        <term_definitions code="at1059">
                            <items id="text">Kommentar</items>
                            <items id="description">Ytterligare beskrivning av fynden frn puls eller hjrtslag som inte beskrivits i andra flt.</items>
                        </term_definitions>
