                        <term_definitions code="at0000">
                            <items id="text">Andning</items>
                            <items id="description">De observerade egenskaperna i spontanandning.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Historik</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Ospecificerad hndelse</items>
                            <items id="description">Ospecificerad standardhndelse vid en tidpunkt eller inom ett tidsintervall som explicit kan definieras i en mall eller genereras automatiskt av vissa IT-system.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">List</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Frekvens</items>
                            <items id="description">Spontanandningens frekvens.</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Regelbundenhet</items>
                            <items id="description">Spontanandningens regelbundenhet.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Regelbunden</items>
                            <items id="description">Regelbunden andning.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Oregelbunden</items>
                            <items id="description">Oregelbunden andning.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Klinisk tolkning</items>
                            <items id="description">Ord, fras eller kort beskrivning som fngar den kliniska betydelsen hos andningsfynden.</items>
                            <items id="comment">Kodning med en terminologi rekommenderas dr s r mjligt. Exempelvis normal andning, tecken p anstrngd andning, Kussmauls andning, Cheyne-Stokes andning, terkommande apn. Flera val r tilltet.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Andningsdjup</items>
                            <items id="description">Spontanandningens djup.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Normal</items>
                            <items id="description">Normalt andningsdjup.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Ytlig</items>
                            <items id="description">Ytligt andningsdjup.</items>
                        </term_definitions>
                        <term_definitions code="at0019">
                            <items id="text">Djup</items>
                            <items id="description">Djupandning.</items>
                        </term_definitions>
                        <term_definitions code="at0022">
                            <items id="text">List</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0024">
                            <items id="text">Klinisk beskrivning</items>
                            <items id="description">Beskrivning av individens andningsdjup.</items>
                            <items id="comment">Exempelvis plgad andning, andningsrrelse med hjlp av accessorisk muskel eller intermittent apn; suckar, grymtningar, jmmer, flmtningar. Nr en fullstndig respiratorisk underskning genomfrs dokumenteras den lmpligen som en del av underskningsfynden.</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="text">Ojmn</items>
                            <items id="description">Ojmnt andningsdjup.</items>
                        </term_definitions>
                        <term_definitions code="at0037">
                            <items id="text">Anstrngning</items>
                            <items id="description">Detaljer om den fysiska anstrngningen vid underskningen.</items>
                            <items id="comment">Individens anstrngningsniv under eller strax innan observationen.</items>
                        </term_definitions>
                        <term_definitions code="at0055">
                            <items id="text">Tillfrd syrgas</items>
                            <items id="description">Detaljer om mngden syrgas som tillfrs individen vid observationstillfllet.</items>
                            <items id="comment">Vrden som antas r syrekoncentration p 21%, Fi02 p 0.21 och fldeshastighet av syre p 0 l/min eller 0 ml/min.</items>
                        </term_definitions>
                        <term_definitions code="at0056">
                            <items id="text">Mjliga felkllor</items>
                            <items id="description">Beskrivning av faktorer och felkllor som kan pverka bedmningen av andningen.&#13;
</items>
                            <items id="comment">Exempelvis ngestniv, smrta, matning av spdbarn, trakeostomi.</items>
                        </term_definitions>
                        <term_definitions code="at0057">
                            <items id="text">*Tree(en)</items>
                            <items id="description">*@ internal @(en)</items>
                        </term_definitions>
                        <term_definitions code="at0058">
                            <items id="text">Tillggsinformation</items>
                            <items id="description">Plats fr att infoga tillggsinformation som krvs fr lokala anpassningar eller anpassning till andra referensmodeller eller formella krav.</items>
                            <items id="comment">Exempelvis lokala informationskrav eller metadata fr anpassning till FHIR.</items>
                        </term_definitions>
                        <term_definitions code="at0062">
                            <items id="text">Frekomst</items>
                            <items id="description">Observation av spontanandning.</items>
                            <items id="comment">Det hr fltet kan vara anvndbart i extrema situationer dr formell mtning av andning ej r mjlig. Exempelvis om patienten r medvetsls och befinner sig p annan plats, ssom fast i en bil eller har fallit utfr en klippa. Frekomst av andning kan antas om adningsfrekvensen r hgre n 0 andetag per minut.</items>
                        </term_definitions>
                        <term_definitions code="at0063">
                            <items id="text">Frekommer</items>
                            <items id="description">Andningsrrelser observeras.</items>
                        </term_definitions>
                        <term_definitions code="at0064">
                            <items id="text">Ej mjlig att upptcka</items>
                            <items id="description">Andningsrrelser kan inte upptckas genom observation.</items>
                        </term_definitions>
                        <term_definitions code="at0065">
                            <items id="text">Kroppsstllning</items>
                            <items id="description">Individens kroppsstllning vid observationen.</items>
                        </term_definitions>
                        <term_definitions code="at0066">
                            <items id="text">Stende</items>
                            <items id="description">Individen stod, gick eller sprang.</items>
                        </term_definitions>
                        <term_definitions code="at0067">
                            <items id="text">Sittande</items>
                            <items id="description">Individen satt p exempelvis en sng eller en stol.</items>
                        </term_definitions>
                        <term_definitions code="at0068">
                            <items id="text">Lutande</items>
                            <items id="description">Individen lutade sig i ungefr 45 graders vinkel med benen i hglge i bckenniv.</items>
                        </term_definitions>
                        <term_definitions code="at0069">
                            <items id="text">Liggande</items>
                            <items id="description">Individen lg p rygg.</items>
                        </term_definitions>
                        <term_definitions code="at0070">
                            <items id="text">Kommentar</items>
                            <items id="description">Kommentarer avseende andning som inte beskrivs i vriga flt.</items>
                        </term_definitions>
                        <term_definitions code="at0071">
                            <items id="text">Maglge</items>
                            <items id="description">Individen lg p mage.</items>
                        </term_definitions>
