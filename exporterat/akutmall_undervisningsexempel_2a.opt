                <items id="description">The systolic blood pressure measurement is greater than/equal to 220 mmHg.</items>
            </items>
            <items code="at0068">
                <items id="text">91-110</items>
                <items id="description">The pulse measurement is between 91 and 110 beats/min.</items>
            </items>
        </term_definitions>
        <term_definitions language="de">
            <items code="at0000">
