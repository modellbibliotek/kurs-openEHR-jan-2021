                <items id="description">Category for the pulse measurement.</items>
            </items>
            <items code="at0006">
                <items id="text">Respiration rate</items>
                <items id="description">Category for the respiratory rate measurement.</items>
            </items>
            <items code="at0007">
                <items id="text">Temperature</items>
                <items id="description">Range category for the body temperature measurement.</items>
            </items>
            <items code="at0008">
