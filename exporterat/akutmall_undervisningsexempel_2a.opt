    <component_ontologies archetype_id="openEHR-EHR-OBSERVATION.news2.v1">
        <term_definitions language="en">
            <items code="at0000">
                <items id="text">NEWS2 Score</items>
                <items id="description">A simple assessment score used to identify clinical deterioration in a patient.</items>
                <items id="comment">This is the second version of the National Early Warning Score (NEWS), issued by the UK Royal College of Physicians in 2017.</items>
            </items>
            <items code="at0001">
                <items id="text">Event Series</items>
                <items id="description">@ internal @</items>
            </items>
            <items code="at0002">
                <items id="text">Any point in time event</items>
                <items id="description">Default, unspecified point in time event which may be explicitly defined in a template or at run-time.</items>
            </items>
            <items code="at0003">
                <items id="text">Tree</items>
                <items id="description">@ internal @</items>
            </items>
            <items code="at0004">
                <items id="text">Systolic blood pressure</items>
                <items id="description">Category for the systolic blood pressure measurement.</items>
            </items>
            <items code="at0005">
