                <items id="description">The respiratory rate measurement is between 12 and 20 breaths/min.</items>
            </items>
            <items code="at0019">
                <items id="text">9-11</items>
                <items id="description">The respiratory rate measurement is between 9 and 11 breaths/min.</items>
            </items>
            <items code="at0020">
                <items id="text">21-24</items>
