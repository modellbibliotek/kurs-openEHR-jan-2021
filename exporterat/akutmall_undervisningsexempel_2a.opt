                        <term_definitions code="at0000">
                            <items id="text">NEWS2-bedömning</items>
                            <items id="description">*A simple assessment score used to identify clinical deterioration in a patient. (en)&#13;
ett hjälpmedel för att bedöma patientens vitala funktioner&#13;
</items>
                            <items id="comment">*This is the second version of the National Early Warning Score (NEWS), issued by the UK Royal College of Physicians in 2017. (en)</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Event Series</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">*Any point in time event (en)</items>
                            <items id="description">*Default, unspecified point in time event which may be explicitly defined in a template or at run-time. (en)</items>
                        </term_definitions>
                        <term_definitions code="at0003">
