                <items id="description">The oxygen saturation level is between 86% and 87%.</items>
            </items>
            <items code="at0050">
                <items id="text">84-85</items>
                <items id="description">The oxygen saturation level is between 84% and 85%.</items>
            </items>
            <items code="at0051">
                <items id="text">97 on oxygen</items>
                <items id="description">The oxygen saturation level is greater than/equal to 97% on supplemental oxygen.</items>
            </items>
            <items code="at0056">
                <items id="text">Clinical risk category</items>
                <items id="description">Overall category representing the urgency and scale of the clinical response required in response to the physiological variables.</items>
            </items>
            <items code="at0057">
                <items id="text">Low</items>
                <items id="description">Ward-based response.</items>
            </items>
            <items code="at0058">
