                        <term_definitions code="at0000">
                            <items id="text">Pulsoximetri</items>
                            <items id="description">Syremttnad i blodet och relaterade mtningar, uppmtta med pulsoximetri eller CO-oximetri.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Hndelsefrlopp</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Ospecificerad hndelse</items>
                            <items id="description">Ospecificerad hndelse&#13;
&#13;
Ospecificerad standardhndelse vid en tidpunkt eller inom ett tidsintervall som explicit kan definieras i en mall eller genereras automatiskt av vissa IT-system.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">SpO</items>
                            <items id="description">Syremttnad i perifert blod uppmtt genom pulsoximetri.</items>
                            <items id="comment">SpO defineras som procentandelen oxyhemoglobin (HbO) i frhllande till totala koncentrationen av hemoglobin (HbO + deoxyhemoglobin) i perifert blod.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">List</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Mtplats</items>
                            <items id="description">Plats p kroppen dr sensorn satt under mtningen</items>
                            <items id="comment">Exempelvis: hger pekfinger, vnster rsnibb.&#13;
Br kodas med terminologi om mjligt</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0015">
                            <items id="text">Tillfrd syrgas</items>
                            <items id="description">Detaljer om mngden syre som patienten inandas vid observationstillfllet.&#13;
</items>
                            <items id="comment">Antagna vrden p 21% syrekoncentration, Fi02 p 0,21 och tillfrt extra syrgasflde 0 l/min eller 0 ml/min.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Mjliga felkllor</items>
                            <items id="description">Beskrivning av faktorer och felkllor som kan pverka bedmningen av mtningen.</items>
                            <items id="comment">Exempelvis rrelse, smrta, dlig perfusion, spdbarn som ammar, perifer hypotermi, bedvning etc.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Oximetriutrustning</items>
                            <items id="description">Detaljer om den icke-invasiva oximetriutrustningen som anvndes.</items>
                        </term_definitions>
                        <term_definitions code="at0034">
                            <items id="text">Anstrngning</items>
                            <items id="description">Detaljer om fysisk aktivitet vid mtningstillfllet.</items>
                        </term_definitions>
                        <term_definitions code="at0036">
                            <items id="text">Kommentar</items>
                            <items id="description">Kommentar angende pulsoximetriresultatet</items>
                        </term_definitions>
                        <term_definitions code="at0044">
                            <items id="text">SpOC</items>
                            <items id="description">Syreinehllet i preifert blod, berknat frn pulsoximetri och puls-CO-oximetri i kombination.</items>
                        </term_definitions>
                        <term_definitions code="at0045">
                            <items id="text">SpCO</items>
                            <items id="description">Mttnaden av karboxihemoglobin i perifert blod, mtt genom puls CO-oximetri.</items>
                        </term_definitions>
                        <term_definitions code="at0046">
                            <items id="text">SpMet</items>
                            <items id="description">Mttnaden av metahemoglobin i perifert blod uppmtt genom puls-CO-oximetri.</items>
                        </term_definitions>
                        <term_definitions code="at0054">
                            <items id="text">Vgform</items>
                            <items id="description">Vgform kopplad till oximetrimtningen.</items>
                        </term_definitions>
                        <term_definitions code="at0058">
                            <items id="text">Tolkning</items>
                            <items id="description">Ord, fras eller kort text som beskriver den kliniska innebrden och signifikansen av mtningarna.</items>
                            <items id="comment">Br kodas med terminologi om mjligt, t.ex. kodade termer fr "normal syremttnad" eller "hypoxi"</items>
                        </term_definitions>
                        <term_definitions code="at0059">
                            <items id="text">Tillggsinformation</items>
                            <items id="description">Plats fr att infoga tillggsinformation som krvs fr lokala anpassningar eller anpassning till andra referensmodeller eller formella krav.</items>
                            <items id="comment">Exempelvis lokala informationskrav eller metadata fr anpassning till HL7 eller CDISC-motsvarigheter.</items>
                        </term_definitions>
                        <term_definitions code="at0060">
                            <items id="text">Multimedia-bild(er)</items>
                            <items id="description">Detaljer fr en serie oximetrimtningar, utver vgformer, representerade som en bild eller bildserie. Rena vgformer skall istllet lagras i fltet "Vgform" och tillhrande CLUSTER-arketyp.</items>
                        </term_definitions>
                        <term_definitions code="at0061">
                            <items id="text">Pre/post-duktal</items>
                            <items id="description">Mtsensorns placering i relation till ductus arteriosus hos nyfdda &#13;
barn, fr att avgra om blodtillfrseln till kroppsdelen sensorn sitter p r pre- eller post-duktal fr patienter med ppetstende ductus arteriosus.</items>
                        </term_definitions>
                        <term_definitions code="at0062">
                            <items id="text">Preduktal</items>
                            <items id="description">Sensorn r placerad preduktalt.</items>
                        </term_definitions>
                        <term_definitions code="at0063">
                            <items id="text">Postduktal</items>
                            <items id="description">Sensorn r placerad postduktalt.</items>
                        </term_definitions>
                        <term_definitions code="at0064">
                            <items id="text">Obestmd</items>
                            <items id="description">Det gr inte att bedma om sensorplatsen r pre- eller post-duktal</items>
                        </term_definitions>
